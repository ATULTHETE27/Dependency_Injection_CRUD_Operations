@model CollageViewModel
@{
    ViewBag.Title = "Edit Collage";
}


<div class="row text">
    <div class="col-md-8 offset-2">
        <p><h1>Update Collage details </h1></p>
        <form asp-controller="Collage" asp-action="EditCollage" method="post" class="mt-3">
            <input hidden asp-for="Id" />
            @*Partial tag helper*@
            <partial name="_CollageFormFields" model="Model" />

        </form>
    </div>
</div>



