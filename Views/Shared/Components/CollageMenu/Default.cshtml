@model IEnumerable<Collage>

<li class="nav-item dropdown">
    <a class="nav-link dropdown-toggle" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false" asp-controller="Collage" asp-action="Index">
        Collages
    </a>
    <ul class="dropdown-menu" aria-labelledby="navbarDarkDropdownMenuLink">
        @foreach (var collage in Model)
        {
            <li><a class="dropdown-item" asp-Controller="Student" asp-action="DisplayStudentsByCollageId" asp-route-id="@collage.Id">@collage.Collage_Name</a></li>
        }
    </ul>
</li>